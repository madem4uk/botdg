ALLOWED_CHAT_IDS=-1002685602852,-4649902952

# Filter settings (optional)
# BANK_FILTER=t
# REQUISITE_FILTER=+
# MIN_AMOUNT=38000

# TDLib settings
TDLIB_DATA_DIR=tdlib_data 